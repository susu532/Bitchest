(function($) {
    if (typeof $ === 'undefined') {
        return;
    }

    $(document).on('gform_post_render', function(event, form_id) {

        const target_form_id = 43;
        if (form_id != target_form_id) {
            return;
        }
        
        const wrapper = '#gform_wrapper_' + form_id;
        
        // --- 1. Блокируем СТАТИЧЕСКИЕ поля (18 и 24) ---
        // Эти поля всегда на месте, их можно блокировать сразу.
        const static_fields = ['18', '24'];
        static_fields.forEach(function(field_id) {
            let selector = wrapper + ' #field_' + form_id + '_' + field_id;
            $(selector).css({'opacity': '0.6', 'pointer-events': 'none'});
            $(selector).find('input, textarea, select, button').prop('disabled', true);
        });

        // --- 2. Блокируем ДИНАМИЧЕСКОЕ поле загрузки (12) с помощью MutationObserver ---
        const uploader_field_id = 12;
        const uploader_container_selector = wrapper + ' #field_' + form_id + '_' + uploader_field_id;
        const uploader_container = $(uploader_container_selector);

        if (uploader_container.length > 0) {
            
            // Функция, которая выполняет блокировку
            const disableUploader = function() {
                const uploader_button = uploader_container.find('.gpfup__select-files');
                
                // Если кнопка найдена...
                if (uploader_button.length > 0) {
                    // ...блокируем всё и отключаем наблюдателя, так как работа сделана.
                    uploader_container.find('.gpfup__droparea').css({'opacity': '0.6', 'pointer-events': 'none'});
                    uploader_button.prop('disabled', true);
                    uploader_container.find('input[type="file"]').prop('disabled', true);
                    
                    if (observer) {
                        observer.disconnect();
                    }
                    return true; // Сообщаем, что блокировка удалась
                }
                return false; // Кнопка еще не появилась
            };

            // Создаем наблюдателя, который будет вызывать нашу функцию при изменениях
            const observer = new MutationObserver(disableUploader);

            // Начинаем наблюдение за контейнером поля и всеми его дочерними элементами
            observer.observe(uploader_container[0], {
                childList: true,
                subtree: true
            });

            // Пробуем заблокировать сразу, на случай если кнопка уже на месте
            disableUploader();
        }
    });
})(jQuery);
