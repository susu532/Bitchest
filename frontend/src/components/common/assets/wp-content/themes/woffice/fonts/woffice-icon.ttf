        <div class="mycred-popup-alert" style="display:none; position: fixed; z-index: 99999;"></div>

        <script type="text/javascript">
            var msp_config = {"base_interval":10000,"ajax_url":"https:\/\/3d-stuff.community\/wp-admin\/admin-ajax.php","title_tpl":"%username%","desc_tpl":"%creds% %ctype%","meta_id":0};

            var msp_state = {
                loading: false,
                high_water_mark: 0,
                shown_history: [],
                max_history_len: 100, // Помним последние 100, чтобы не повторяться
                total_counter: 0
            };

            jQuery(document).ready(function($) {
                var cookie_id = parseInt(getCookie('social_proof_log_id')) || 0;
                msp_state.high_water_mark = Math.max(cookie_id, msp_config.meta_id);

                console.log('MSP: Ready. Watermark:', msp_state.high_water_mark);
                get_logs();
            });

            function get_logs() {
                if (msp_state.loading) return;
                msp_state.loading = true;

                jQuery.post(msp_config.ajax_url, {
                    'action': 'msp_popup_ajax',
                    'last_log_id': msp_state.high_water_mark
                }, function(response) {
                    msp_state.loading = false;

                    // Парсинг ответа
                    var transactions = [];
                    try {
                        if (!response) throw new Error("Empty");
                        transactions = typeof response === 'object' ? response : JSON.parse(response);
                    } catch (e) {
                        schedule_next(30000);
                        return;
                    }

                    if (transactions.length === 0) {
                        schedule_next(msp_config.base_interval * 2);
                        return;
                    }

                    // 1. Обновляем Watermark (только вверх)
                    var max_id = 0;
                    transactions.forEach(function(t) {
                        if (parseInt(t.id) > max_id) max_id = parseInt(t.id);
                    });

                    if (max_id > msp_state.high_water_mark) {
                        msp_state.high_water_mark = max_id;
                        safeSetCookie('social_proof_log_id', max_id, 365);
                        console.log('MSP: 📈 New Watermark:', max_id);
                    }

                    // 2. Умная фильтрация на клиенте
                    var incoming_new = [];
                    var incoming_old = [];

                    transactions.forEach(function(item) {
                        if (item.is_new === true) {
                            incoming_new.push(item);
                        } else {
                            // Берем в пул старых только то, что не видели
                            if (!msp_state.shown_history.includes(parseInt(item.id))) {
                                incoming_old.push(item);
                            }
                        }
                    });

                    // Перемешиваем доступные старые
                    incoming_old.sort(function() { return 0.5 - Math.random() });

                    // Берем топ-5 из перемешанных старых
                    var selected_old = incoming_old.slice(0, 5);

                    // Очередь: Сначала НОВЫЕ (все), потом выбранные СТАРЫЕ
                    var queue = incoming_new.concat(selected_old);

                    if (queue.length === 0) {
                        // Если совсем ничего не нашли (пользователь видел всё из последних 60)
                        console.log('MSP: All available logs viewed. Waiting...');
                        schedule_next(msp_config.base_interval * 1.5);
                        return;
                    }

                    // Добавляем выбранное в историю
                    queue.forEach(function(item) {
                        addToHistory(parseInt(item.id));
                    });

                    console.log('MSP: 📥 Processing batch. Queue size: ' + queue.length + ' (New: ' + incoming_new.length + ', Old: ' + selected_old.length + ')');
                    
                    run_display_loop(queue, 0);

                }).fail(function() {
                    msp_state.loading = false;
                    schedule_next(30000);
                });
            }

            function addToHistory(id) {
                msp_state.shown_history.push(id);
                if (msp_state.shown_history.length > msp_state.max_history_len) {
                    msp_state.shown_history.shift();
                }
            }

            function run_display_loop(list, index) {
                if (index >= list.length) {
                    schedule_next(msp_config.base_interval);
                    return;
                }

                var item = list[index];
                var is_new = (item.is_new === true);

                // ЗАДЕРЖКИ
                var delay = msp_config.base_interval;
                if (!is_new) {
                    // Старые: 15 сек + (0..25 сек рандом)
                    delay += 15000 + (Math.random() * 25000);
                }

                console.log('MSP: Popup #' + (msp_state.total_counter + 1) + ' | ' + (is_new ? '⚡ NEW' : '📚 OLD') + ' (ID:' + item.id + ') | Next in: ' + (delay/1000).toFixed(1) + 's');

                setTimeout(function() {
                    show_popup(item);
                    msp_state.total_counter++;
                    run_display_loop(list, index + 1);
                }, delay);
            }

            function schedule_next(delay) {
                setTimeout(function() { get_logs(); }, delay);
            }

            function show_popup(obj) {
                var title = replace_tags(msp_config.title_tpl, obj);
                var desc = replace_tags(msp_config.desc_tpl, obj);
                
                // Стилевые классы
                var theme_cls = 'mpa-theme-default'; // дефолт
                var border_cls = '';
                var duration = 4000;

                if (typeof mycred_popup !== 'undefined') {
                    if(mycred_popup.theme) theme_cls = mycred_popup.theme;
                    if(mycred_popup.border_style) border_cls = mycred_popup.border_style;
                    if(mycred_popup.on_screen_time) duration = parseInt(mycred_popup.on_screen_time);
                }

                // Строгая структура HTML для CSS
                var html = '<div class="mpa-alert ' + theme_cls + ' ' + border_cls + '">' +
                           '<div class="mpa-left"><div class="mpa-avatar"><img src="' + obj.user_avatar + '" width="50px"/></div></div>' +
                           '<div class="mpa-right"><div class="mpa-title">' + title + '</div><div class="mpa-description">' + desc + '</div></div>' +
                           '</div>';

                var $alert = jQuery('.mycred-popup-alert');
                $alert.html(html);

                // Анимация
                $alert.stop(true, true).show().css('bottom', '-100px').animate({ bottom: "5%" }, 1000);

                setTimeout(function() {
                    $alert.animate({ bottom: "-50%" }, 1000, function() {
                        $alert.hide();
                    });
                }, duration);
            }

            function replace_tags(str, obj) {
                if (!str) return '';
                str = str.replace(/%username%/g, "<span class='msp-username'>" + obj.user_name + "</span>");
                str = str.replace(/%creds%/g, "<span class='msp-creds'>" + obj.creds + "</span>");
                str = str.replace(/%ctype%/g, "<span class='msp-pointtype'>" + obj.point_type + "</span>");
                str = str.replace(/%reference%/g, "<span class='msp-reference'>" + obj.reference + "</span>");
                str = str.replace(/%entry%/g, "<span class='msp-entry'>" + obj.entry + "</span>");
                return str.replace(/_/g, ' ');
            }

            function safeSetCookie(cname, cvalue, exdays) {
                var current = parseInt(getCookie(cname)) || 0;
                if (parseInt(cvalue) < current) return;
                var d = new Date();
                d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
                document.cookie = cname + "=" + cvalue + ";expires=" + d.toUTCString() + ";path=/";
            }

            function getCookie(name) {
                var v = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
                return v ? v[2] : null;
            }
        </script>
