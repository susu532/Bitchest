<script>
document.addEventListener('DOMContentLoaded', function() {
    // Добавляем класс "lightbox" ко всем изображениям
    const images = document.querySelectorAll('.blog-content img');
    images.forEach(image => {
        image.classList.add('lightbox');
    });

    // Логика для лайтбокса
    images.forEach(image => {
        image.addEventListener('click', function() {
            const fullSizeUrl = this.src;

            // Создаем лайтбокс
            const lightbox = document.createElement('div');
            lightbox.className = 'lightbox-overlay';

            // Создаем изображение
            const img = new Image();
            img.src = fullSizeUrl;
            img.className = 'lightbox-image';

            // Добавляем изображение в лайтбокс
            lightbox.appendChild(img);
            document.body.appendChild(lightbox);

            // Переменные для масштабирования и перемещения
            let scale = 1;
            const scaleStep = 0.1;
            let isDragging = false;
            let startX, startY, translateX = 0, translateY = 0;

            // Обработка колеса мыши для масштабирования
            lightbox.addEventListener('wheel', function(event) {
                event.preventDefault();

                if (event.deltaY < 0) {
                    scale += scaleStep;
                } else {
                    scale -= scaleStep;
                }

                scale = Math.min(Math.max(scale, 0.5), 3); // Ограничиваем масштаб
                updateTransform();
            });

            // Обработка событий мыши для перетаскивания
            img.addEventListener('mousedown', function(event) {
                event.preventDefault(); // Предотвращаем действие по умолчанию
                isDragging = true;
                startX = event.clientX - translateX;
                startY = event.clientY - translateY;
                img.style.cursor = 'grabbing'; // Меняем курсор при перетаскивании
            });

            lightbox.addEventListener('mousemove', function(event) {
                if (isDragging) {
                    event.preventDefault(); // Предотвращаем действие по умолчанию
                    translateX = event.clientX - startX;
                    translateY = event.clientY - startY;
                    updateTransform();
                }
            });

            lightbox.addEventListener('mouseup', function() {
                isDragging = false;
                img.style.cursor = 'grab'; // Возвращаем курсор после перетаскивания
            });

            lightbox.addEventListener('mouseleave', function() {
                isDragging = false;
                img.style.cursor = 'grab'; // Возвращаем курсор, если мышь вышла за пределы
            });

            // Предотвращаем перетаскивание изображения браузером
            img.addEventListener('dragstart', function(event) {
                event.preventDefault();
            });

            // Центрирование изображения по двойному щелчку
            img.addEventListener('dblclick', function(event) {
                event.preventDefault();
                translateX = 0;
                translateY = 0;
                updateTransform();
            });

            // Функция для обновления трансформации
            function updateTransform() {
                img.style.transform = `scale(${scale}) translate(${translateX}px, ${translateY}px)`;
            }

            // Закрытие лайтбокса при клике на затемненную область
            lightbox.addEventListener('click', function(event) {
                if (event.target === lightbox) {
                    document.body.removeChild(lightbox);
                }
            });

            // Обработка ошибок загрузки изображения
            img.onerror = function() {
                console.error('Error loading image:', fullSizeUrl);
                document.body.removeChild(lightbox);
            };
        });
    });
});
    </script>
