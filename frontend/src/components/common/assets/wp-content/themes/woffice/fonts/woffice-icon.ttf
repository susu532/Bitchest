/* --- ФИНАЛЬНЫЕ СТИЛИ ДЛЯ БЛОКА НАГРАД --- */

/* Главный контейнер */
.community-rewards-container {
    font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Helvetica, Arial, sans-serif;
    color: #333;
    line-height: 1.6;
    padding: 20px 0;
    }

/* Общие стили для основных блоков (вступление, бриллианты, баллы) */
.intro-section, .reward-section {
    padding: 30px 40px;
    margin-bottom: 25px;
    border-radius: 12px;
    box-shadow: 0 4px 15px rgba(0, 0, 0, 0.05);
}

/* Блок вступления */
.intro-section {
    background-color: #f8f9fa;
    text-align: center;
    border: 1px solid #e9ecef;
}

.intro-section h2 {
    font-size: 1.2em; /* Немного уменьшен для соответствия скриншоту */
    font-weight: 600;
    color: #495057;
    margin-top: 0;
    margin-bottom: 25px; /* Увеличен отступ после заголовка */
    letter-spacing: 0.5px;
    text-transform: uppercase;
}

/* ИЗМЕНЕНИЕ ЗДЕСЬ: Убрано ограничение ширины */
.intro-section p {
    font-size: 1.2em !important;      /* Текст стал крупнее */
    font-weight: 600 !important;      /* Текст стал жирнее (полужирный) */
    color: #495057;        /* Цвет сделан темнее для лучшей читаемости */
    margin: 0 auto 15px auto;
    line-height: 1.6 !important;
    
}

/* Уведомление о прозрачности */
.transparency-notice {
    background-color: #f0f4f8;
    border-left: 5px solid #6c757d;
    padding: 15px 20px;
    margin: 0 auto 25px auto;
    display: flex;
    align-items: center;
    border-radius: 8px;
    max-width: 850px;
}

.transparency-notice i {
    font-size: 1.6em;
    color: #6c757d;
    margin-right: 15px;
}
.transparency-notice span {
    color: #495057;
    font-weight: 500;
}

/* Заголовок секции (H2) */
.reward-section h2, .intro-section h2 {
    display: flex;
    align-items: center;
    justify-content: center;
}

.reward-section h2 {
    font-size: 1.2em; /* Немного уменьшен для соответствия скриншоту */
    font-weight: 600;
    margin-top: 0;
    margin-bottom: 30px;
    padding-bottom: 15px;
    justify-content: flex-start;
    letter-spacing: 0.5px;
    text-transform: uppercase;
}

/* Иконка в заголовке */
.reward-section h2 i, .intro-section h2 i {
    margin-right: 12px;
    font-size: 1.2em; /* Увеличена иконка относительно текста */
}

/* Отдельный пункт в списке */
.reward-item {
    display: flex;
    align-items: flex-start;
    margin-bottom: 25px;
}
.reward-item:last-child { margin-bottom: 0; }

.reward-icon {
    font-size: 1.8em;
    width: 50px;
    margin-right: 20px;
    text-align: center;
    padding-top: 4px;
}

.reward-content h4 {
    margin: 0 0 5px 0;
    font-size: 1.2em;
    font-weight: 600;
}

.reward-content p {
    margin: 0;
    font-size: 1em;
    opacity: 0.9;
}


/* --- Цветовые схемы --- */

/* Секция Diamonds (Зеленая) */
.diamonds-section {
    background-color: #f1f8e9;
    color: #82b440;
}
.diamonds-section h2 {
    border-bottom: 2px solid rgba(130, 180, 64, 0.3);
}
.diamonds-section .reward-content p {
    color: #2e4428;
}

/* Секция Points (Оранжевая) */
.points-section {
    background-color: #fff8e1;
    color: #ffa500;
}
.points-section h2 {
    border-bottom: 2px solid rgba(255, 165, 0, 0.3);
}
.points-section .reward-content p {
    color: #594a2d;
}

/* Адаптивность для мобильных устройств */
@media (max-width: 768px) {
    .intro-section, .reward-section { padding: 20px; }
    .reward-section h2, .intro-section h2 {
        font-size: 1.1em;
        text-align: center;
        justify-content: center;
    }
    .reward-item {
        flex-direction: column;
        align-items: center;
        text-align: center;
    }
    .reward-icon {
        margin-right: 0;
        margin-bottom: 10px;
        font-size: 2.2em;
    }
}

.reward-block-container {
    display: flex;
    justify-content: space-between;
    align-items: center;
    background: linear-gradient(135deg, #ffac00 0%, #ffac00 100%);
    border-radius: 8px;
    padding: 5px 25px;
    width: 100%;
    box-sizing: border-box;
    position: relative;
}

.reward-text-center {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    font-family: 'Arial', sans-serif;
    font-size: 24px;
    font-weight: bold;
    color: #c25d2bf2;
    text-shadow: 1px 1px #f5c41a;
    text-transform: uppercase;
    white-space: nowrap;
}

.pulse-diamond {
    font-weight: 800;
    font-size: 24px;
    animation: pulse 1.5s infinite;
    display: inline-block;
    margin: 0 3px;
    text-shadow: 1px 1px #f5c41a;
}

.reward-btn {
    background: rgba(255, 255, 255, 0.2);
    border-radius: 4px;
    padding: 8px 12px;
    color: white;
    transition: all 0.3s ease;
    margin-left: auto; /* Кнопка прижимается вправо */
    display: flex;
    align-items: center;
    justify-content: center;
}

.reward-btn:hover {
    background: rgba(255, 255, 255, 0.3);
    transform: translateY(-1px);
}

.reward-btn i {
    font-size: 14px;
}

@keyframes pulse {
    0% { transform: scale(1); }
    50% { transform: scale(1.1); }
    100% { transform: scale(1); }
}

/* Адаптивность */
@media (max-width: 768px) {
    .reward-text-center {
        font-size: 14px;
        white-space: normal;
        position: static;
        transform: none;
        text-align: center;
        margin-bottom: 10px;
    }
    
    .reward-block-container {
        flex-direction: column;
        padding: 15px;
    }
    
    .reward-btn {
        margin-left: 0;
    }
}

