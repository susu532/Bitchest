        document.addEventListener('DOMContentLoaded', function() {
            var tabLinks = document.querySelectorAll('.my-custom-tabs .nav-tabs a');
            if(tabLinks.length === 0) return;

            tabLinks.forEach(function(link) {
                link.addEventListener('click', function(e) {
                    e.preventDefault();
                    var tabList = link.closest('ul');
                    var currentActive = tabList.querySelector('li.active');
                    if(currentActive) currentActive.classList.remove('active');
                    link.parentElement.classList.add('active');

                    var targetId = link.getAttribute('href').replace('#', '');
                    // ЗАЩИТА: Простая проверка, что ID состоит только из безопасных символов, чтобы не ломать querySelector
                    if (!/^[a-zA-Z0-9_-]+$/.test(targetId)) return;

                    var wrapper = link.closest('.my-custom-tabs');
                    var panes = wrapper.querySelectorAll('.tab-pane');
                    panes.forEach(function(pane) {
                        pane.classList.remove('active'); pane.classList.remove('in');
                    });

                    var activePane = document.getElementById(targetId);
                    if (activePane) {
                        activePane.classList.add('active');
                        setTimeout(function(){ activePane.classList.add('in'); }, 10);
                    }
                });
            });
        });
