/* 2. СТИЛИ ТОЛЬКО ДЛЯ КВАДРАТНЫХ КНОПОК-ЦИФР */
/* Выбираем все ссылки, КРОМЕ тех, что в .page-nav-left/right */
.celst-pagination .navigation li:not(.page-nav-left):not(.page-nav-right) a {
    min-width: 44px !important;
    padding: 0 10px !important;
}

/* Эффект пробегающей полосы для всех ссылок */
.celst-pagination .navigation li a::before {
    content: '';
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.4), transparent);
    transition: left 0.6s ease;
    z-index: 1;
}
.celst-pagination .navigation li a:hover::before {
    left: 100%;
}

/* Стиль наведения для цифр */
.celst-pagination .navigation li:not(.page-nav-left):not(.page-nav-right) a:hover {
    background: #8bc34a !important;
    color: white !important;
}

/* Активная страница (работает и для a, и для span) */
.celst-pagination .navigation li.active a,
.celst-pagination .navigation li.active span {
    background: #ed8936 !important;
    color: white !important;
    cursor: default !important;
    box-shadow: 0 3px 10px rgba(237, 137, 54, 0.2) !important;
    border-color: #ed8636 !important;
    pointer-events: none !important; /* Делаем неактивным для кликов */
}
.celst-pagination .navigation li.active a:hover,
.celst-pagination .navigation li.active span:hover {
    background: #ed8936 !important; /* Убираем эффект наведения */
}
.celst-pagination .navigation li.active a::before,
.celst-pagination .navigation li.active span::before {
    display: none !important; /* Убираем эффект полосы для активной страницы */
}

/* 3. ИСПРАВЛЕННЫЕ СТИЛИ ДЛЯ КНОПОК "PREVIOUS" и "NEXT" */
.celst-pagination .navigation .page-nav-left a,
.celst-pagination .navigation .page-nav-right a {
    width: auto !important;
    padding: 0 20px !important;
    font-weight: 600 !important;
    text-transform: uppercase !important;
    letter-spacing: 0.5px !important;
    background: #ed8936 !important;
    color: white !important;
    box-shadow: 0 3px 12px rgba(237, 137, 54, 0.3) !important;
    border-color: #ed8936 !important;
    
    /* ИСПРАВЛЕНИЕ: Скрываем исходный текст и показываем только нужный */
    font-size: 0 !important; /* Скрываем исходный текст */
    text-indent: -9999px !important; /* Дополнительное скрытие текста */
}

/* Прячем иконки-стрелки внутри них */
.celst-pagination .navigation .page-nav-left a *,
.celst-pagination .navigation .page-nav-right a * {
    display: none !important;
}

/* ИСПРАВЛЕНИЕ: Добавляем нужный текст через псевдоэлементы */
.celst-pagination .navigation .page-nav-left a::after {
    content: '« Previous' !important;
    font-size: 14px !important;
    text-indent: 0 !important;
    display: inline-block !important;
    letter-spacing: 0.5px !important;
    position: relative !important;
    z-index: 2 !important;
}
.celst-pagination .navigation .page-nav-right a::after {
    content: 'Next »' !important;
    font-size: 14px !important;
    text-indent: 0 !important;
    display: inline-block !important;
    letter-spacing: 0.5px !important;
    position: relative !important;
    z-index: 2 !important;
}

/* Наведение на "Previous" и "Next" */
.celst-pagination .navigation .page-nav-left a:hover,
.celst-pagination .navigation .page-nav-right a:hover {
    transform: translateY(-2px) !important;
    background: #8BC34A !important;
    box-shadow: 0 5px 16px rgba(139, 195, 74, 0.4) !important;
    border-color: #8BC34A !important;
}

/* Точки (многоточие) */
.celst-pagination .navigation li span.disabled {
    background: none !important;
    border: none !important;
    color: #a0aec0 !important;
    cursor: default !important;
    box-shadow: none !important;
    min-width: auto !important;
    width: auto !important;
    height: auto !important;
    padding: 0 8px !important;
}

/* === ДУБЛИРУЮЩИЕ СТИЛИ ДЛЯ ВТОРОГО ТИПА ПАГИНАЦИИ === */
.navigation.pagination {
    margin: 10px 0 10px !important;
    text-align: center !important;
    clear: both !important;
    width: 100% !important;
    display: flex !important;
    justify-content: center !important;
}

.navigation .nav-links {
    display: inline-flex !important;
    background: rgba(255, 255, 255, 0.95) !important;
    border-radius: 5px !important;
    padding: 10px !important;
    box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08) !important;
    gap: 6px !important;
    align-items: center !important;
    justify-content: center !important;
    flex-wrap: wrap !important;
    backdrop-filter: blur(10px) !important;
    border: 1px solid rgba(255, 255, 255, 0.2) !important;
}

.nav-links a,
.nav-links span {
    display: inline-block !important;
    padding: 12px 16px !important;
    text-decoration: none !important;
    color: #4a5568 !important;
    border-radius: 5px !important;
    font-weight: 600 !important;
    font-size: 14px !important;
    letter-spacing: 0.3px !important;
    transition: all 0.3s ease !important;
    border: 1px solid transparent !important;
    position: relative !important;
    overflow: hidden !important;
    min-width: 40px !important;
    text-align: center !important;
    cursor: pointer !important;
    background: transparent !important;
}

/* Эффект пробегающей полосы для всех ссылок */
.nav-links a::before {
    content: '';
    position: absolute;
    top: 0;
    left: -100%;
    width: 100%;
    height: 100%;
    background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.4), transparent);
    transition: left 0.6s ease;
    z-index: 1;
}

.nav-links a:hover::before {
    left: 100%;
}

/* Стиль наведения для цифр */
.nav-links a:not(.prev):not(.next):not(.current):hover {
    background: #8bc34a !important;
    color: white !important;
}

/* Активная страница (работает и для a, и для span) */
.nav-links .current,
.nav-links span.current {
    background: #ed8936 !important;
    color: white !important;
    cursor: default !important;
    box-shadow: 0 3px 10px rgba(237, 137, 54, 0.2) !important;
    transform: none !important;
    border-color: #ed8936 !important;
    pointer-events: none !important; /* Делаем неактивным для кликов */
}
.nav-links .current:hover,
.nav-links span.current:hover {
    background: #ed8936 !important;
    color: white !important;
}
.nav-links .current::before,
.nav-links span.current::before {
    display: none !important; /* Убираем эффект полосы для активной страницы */
}

/* Кнопки "Previous" и "Next" */
.nav-links .prev,
.nav-links .next {
    padding: 12px 20px !important;
    font-weight: 600 !important;
    text-transform: uppercase !important;
    letter-spacing: 0.5px !important;
    background: #ed8936 !important;
    color: white !important;
    box-shadow: 0 3px 12px rgba(237, 137, 54, 0.3) !important;
    border-color: #ed8936 !important;
}

.nav-links .prev:hover,
.nav-links .next:hover {
    transform: translateY(-2px) !important;
    background: #8BC34A !important;
    box-shadow: 0 5px 16px rgba(139, 195, 74, 0.4) !important;
    border-color: #8BC34A !important;
}

/* Точки */
.nav-links .dots {
    background: none !important;
    border: none !important;
    padding: 12px 8px !important;
    color: #a0aec0 !important;
    cursor: default !important;
    transform: none !important;
}

.nav-links .dots:hover {
    background: none !important;
    transform: none !important;
    box-shadow: none !important;
}













