APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Configuration de la base de données
# Pour Bitchest, utilisez MySQL en production
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=bitchest
DB_USERNAME=root
DB_PASSWORD=

# Sessions : utilisez 'database' pour stocker les sessions en BDD (recommandé pour production)
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# Broadcasting : utilisez 'reverb' pour WebSocket en temps réel
BROADCAST_CONNECTION=reverb
FILESYSTEM_DISK=local

# Queue : utilisez 'sync' pour développement (pas besoin de la table jobs)
# En production, utilisez 'redis' ou 'database' si nécessaire
QUEUE_CONNECTION=sync

# Cache : utilisez 'file' pour développement (pas besoin de la table cache)
# En production, utilisez 'redis' pour de meilleures performances
CACHE_STORE=file
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Configuration Reverb pour WebSocket
REVERB_APP_ID=bitchest
REVERB_APP_KEY=bitchest-key
REVERB_APP_SECRET=bitchest-secret
REVERB_HOST=localhost
REVERB_PORT=8080
REVERB_SCHEME=http

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

